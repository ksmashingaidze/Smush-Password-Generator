<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/passbanklayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    android:minWidth="25px"
    android:minHeight="25px">
 

    
    <Button
        android:text="@string/unlock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/textView6"
        app:cornerRadius="30dp"
        android:id="@+id/button1"
        android:enabled="false" />




    <Button
        android:text="@string/delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button1"
        app:cornerRadius="30dp"
        android:id="@+id/button2"
        android:enabled="false" />

    <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:id="@+id/textView5"
        android:textColor="#ffb71c1c" />



    <TextView
        android:text="@string/prompt9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView1" />


    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView1"
        android:layout_centerHorizontal="true"
        android:background="@drawable/spinnerborder"
        android:prompt="@string/cuspassprompt"
        android:id="@+id/spinner1" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/textView1"
        android:id="@+id/editText1"
        android:enabled="false"
        android:visibility="invisible" />



    <TextView
        android:text="@string/question"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_below="@id/editText1"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView2"
        android:visibility="invisible" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/textView2"
        android:id="@+id/editText2"
        android:enabled="false"
        android:visibility="invisible" />

     <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_below="@id/textView2"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView4" />

    <TextView
        android:text="@string/answer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_below="@id/editText2"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView3"
        android:visibility="invisible" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/textView3"
        android:id="@+id/editText3"
        android:enabled="false"
        android:visibility="invisible" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/editText3"
        android:id="@+id/editText4"
        android:enabled="false"
        android:visibility="invisible" />

    <ImageView
        android:src="@drawable/nobodyhome"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:visibility="invisible"
        android:id="@+id/imageView1" />
    
    <!-- <com.google.android.gms.ads.AdView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ads:adUnitId="@string/adunitid"
        ads:adSize="BANNER"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/adView1" /> -->

    <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/editText4"
        android:textSize="@dimen/outputtextsize"
        android:id="@+id/textView6" />









</RelativeLayout>
