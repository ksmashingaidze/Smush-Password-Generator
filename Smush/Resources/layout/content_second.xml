<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:id="@+id/passgenlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    android:minWidth="25px"
    android:minHeight="25px">
 
    <Button
        android:text="@string/generate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/textView6"
        app:cornerRadius="30dp"
        android:id="@+id/button1" />

    <Button
        android:text="@string/save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button1"
        app:cornerRadius="30dp"
        android:id="@+id/button2"
        android:enabled="false" />

    <EditText
        android:inputType="number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/textView1"
        android:id="@+id/editText1" />

    <EditText
        android:inputType="number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/textView2"
        android:id="@+id/editText2" />

    <EditText
        android:inputType="number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/textView3"
        android:id="@+id/editText3" />

    <EditText
        android:inputType="number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/textView5"
        android:layout_below="@id/textView4"
        android:id="@+id/editText4" />

    <TextView
        android:text="@string/prompt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView1" />


    <TextView
        android:text="@string/prompt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_below="@id/editText1"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView2" />

    <TextView
        android:text="@string/prompt3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_below="@id/editText2"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView3" />
    
    <TextView
        android:text="@string/prompt4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_below="@id/editText3"
        android:layout_toRightOf="@id/textView5"
        android:id="@+id/textView4" />

    <TextView
        android:text="@string/essential"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:id="@+id/textView5"
        android:textColor="#ffb71c1c" />

    <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/editText4"
        android:textSize="@dimen/outputtextsize"
        android:id="@+id/textView6" />

    <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/button1"
        android:id="@+id/textView7" />

    <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView7"
        android:id="@+id/textView8" />

    <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView8"
        android:id="@+id/textView9" />

    <!-- <com.google.android.gms.ads.AdView
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ads:adUnitId="@string/adunitid"
        ads:adSize="BANNER"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/adView1" /> -->


    <TextView
        android:text="@string/emptystring"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView9"
        android:id="@+id/textView10" />



</RelativeLayout>
